{
    "compileOnSave": true,
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es2015",
      "module": "CommonJS",
      "moduleResolution": "node",
      "sourceMap": true,
      "declaration": true,
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "preserveWatchOutput": true
    },
  }