{
  "name": "playwright-i18next-fixture",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts --dts",
    "lint": "eslint ./src",
    "lint:fix": "pnpm lint --fix",
    "lint:types": "tsc --noEmit",
    "test": "playwright test",
    "prepublishOnly": "pnpm build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cubanducko/playwright-i18next-fixture.git"
  },
  "author": "Ricardo Dom√≠nguez <rdominguez.alvarez.dev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/cubanducko/playwright-i18next-fixture#readme",
  "bugs": {
    "url": "https://github.com/cubanducko/playwright-i18next-fixture/issues"
  },
  "keywords": [
    "playwright",
    "fixtures",
    "i18next",
    "i18n",
    "utility"
  ],
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@types/jest": "29.0.3",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "ts-jest": "29.0.2",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "@playwright/test": "^1.26.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm lint && pnpm build && pnpm test"
    }
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": "rc"
      }
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "lint-staged": {
    "src/**/**/*.{ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "i18next": "^21.9.2"
  }
}
